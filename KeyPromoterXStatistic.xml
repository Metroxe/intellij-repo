<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Column Selection Mode">
        <Item>
          <IdeaActionID>EditorToggleColumnMode</IdeaActionID>
          <ShortCut>'⇧⌘8' or '⇧⌘⌨ *'</ShortCut>
          <Description>Column Selection Mode</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Commit...">
        <Item>
          <IdeaActionID>CheckinProject</IdeaActionID>
          <ShortCut>'⌘K'</ShortCut>
          <Description>Commit...</Description>
          <Count>1</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy">
        <Item>
          <IdeaActionID>$Copy</IdeaActionID>
          <ShortCut>'⌘C'</ShortCut>
          <Description>Copy</Description>
          <Count>19</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Path">
        <Item>
          <IdeaActionID>CopyPaths</IdeaActionID>
          <ShortCut>'⇧⌘C'</ShortCut>
          <Description>Copy Path</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Copy Relative Path">
